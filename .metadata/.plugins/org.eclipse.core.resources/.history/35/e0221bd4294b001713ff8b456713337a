import java.util.Random;

public class IntenseTaskGenerator implements TaskGenerator {
	private int TMT;
	private int AMOUNT;
	private int nProcessors;
	private static int time = 0;
	private static int upperBound = 10000;
	private static int lowerBound = 1000;
	public IntenseTaskGenerator (int tmt, int amount, int nProc) {
		TMT = tmt;
		AMOUNT = amount;
		nProcessors = nProc;
	}
	public Task generateTask(int cpuID, int startTime) {
		Random random = new Random();
		int randomNumber = random.nextInt(upperBound - lowerBound) + lowerBound;
		
	}

}
